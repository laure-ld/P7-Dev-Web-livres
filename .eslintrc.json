{
    "env": {
        "browser": false,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": "off",
        "prefer-template": "error",
        "arrow-parens": ["error", "always"]
    }
}
